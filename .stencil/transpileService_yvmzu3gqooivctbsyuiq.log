{"moduleFile":{"sourceFilePath":"/Users/johnbreau/github/books-app-stencil/src/components/books-component/discog-component.tsx","localImports":[],"externalImports":["@stencil/core"],"potentialCmpRefs":[],"cmpMeta":{"tagNameMeta":"discogs-component","stylesMeta":{"$":{"externalStyles":[{"absolutePath":"/Users/johnbreau/github/books-app-stencil/src/components/books-component/discog-component.css","cmpRelativePath":"discog-component.css","originalComponentPath":"discog-component.css"}]}},"assetsDirsMeta":[],"hostMeta":{},"dependencies":[],"jsdoc":{"name":"DiscogsComponent","tags":[],"documentation":"","type":"typeof DiscogsComponent"},"encapsulationMeta":0,"componentClass":"DiscogsComponent","membersMeta":{"discogContent":{"memberType":16},"artistId":{"memberType":16},"maxValue":{"memberType":1,"attribName":"max-value","attribType":{"text":"number","required":false,"optional":false,"typeReferences":{}},"reflectToAttrib":false,"propType":8,"jsdoc":{"name":"maxValue","tags":[],"documentation":"","type":"number","default":"5"}},"value":{"memberType":1,"attribName":"value","attribType":{"text":"number","required":false,"optional":false,"typeReferences":{}},"reflectToAttrib":false,"propType":8,"jsdoc":{"name":"value","tags":[],"documentation":"","type":"number","default":"0"}}},"eventsMeta":[],"listenersMeta":[]},"hasSlot":false,"hasSvg":false,"jsFilePath":"/Users/johnbreau/github/books-app-stencil/src/components/books-component/discog-component.js"},"jsText":"export class DiscogsComponent {\r\n    constructor() {\r\n        this.DISCOGS_BASE_API = 'https://api.discogs.com/artists/';\r\n        this.discogContent = [];\r\n        this.maxValue = 5;\r\n        this.value = 0;\r\n    }\r\n    // This will fire the getDiscogData mehtod onload...\r\n    // componentWillLoad() {\r\n    //  this.getDiscogData();\r\n    // }\r\n    createStarList() {\r\n        let starList = [];\r\n        for (let i = 1; i <= this.maxValue; i++) {\r\n            if (i <= this.value) {\r\n                starList.push(h(\"span\", { class: \"rating\" }, \"\\u2605\"));\r\n            }\r\n            else {\r\n                starList.push(h(\"span\", { class: \"rating\" }, \"\\u2606\"));\r\n            }\r\n        }\r\n        return starList;\r\n    }\r\n    handleSubmit(e) {\r\n        e.preventDefault();\r\n        this.getDiscogData(this.artistId);\r\n    }\r\n    handleChange(event) {\r\n        this.artistId = event.target.value;\r\n    }\r\n    getDiscogData(artistId) {\r\n        fetch(this.DISCOGS_BASE_API + artistId)\r\n            .then((response) => response.json())\r\n            .then(response => {\r\n            this.discogContent = response;\r\n        });\r\n    }\r\n    render() {\r\n        return (h(\"div\", { padding: true },\r\n            h(\"form\", { onSubmit: (e) => this.handleSubmit(e) },\r\n                h(\"label\", { htmlFor: \"artist-id\" }, \"Artist ID:\"),\r\n                h(\"input\", { value: this.artistId, onInput: (event) => this.handleChange(event) })),\r\n            h(\"div\", null,\r\n                h(\"h1\", { class: \"header-copy\" }, this.discogContent.name),\r\n                h(\"div\", null, this.createStarList())),\r\n            h(\"div\", { class: \"bio-content\" },\r\n                h(\"p\", { class: \"body-copy\" }, this.discogContent.profile),\r\n                h(\"img\", { class: \"body-image\", src: \"{ this.discogContent.images[0].resource_url }\" }))));\r\n    }\r\n    static get is() { return \"discogs-component\"; }\r\n    static get properties() { return {\r\n        \"artistId\": {\r\n            \"state\": true\r\n        },\r\n        \"discogContent\": {\r\n            \"state\": true\r\n        },\r\n        \"maxValue\": {\r\n            \"type\": Number,\r\n            \"attr\": \"max-value\"\r\n        },\r\n        \"value\": {\r\n            \"type\": Number,\r\n            \"attr\": \"value\"\r\n        }\r\n    }; }\r\n    static get style() { return \"/**style-placeholder:discogs-component:**/\"; }\r\n}\r\n"}